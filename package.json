    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.3",
    "@reduxjs/toolkit": "^1.9.5",
